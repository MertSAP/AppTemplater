{
  "name": "apptemplater",
  "version": "1.0.0",
  "description": "A CAP App for Templating Applications",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js/sqlite": "^1.3.1",
    "@sap/cds": ">=6.7.0",
    "express": "^4"
  },
  "devDependencies": {
    "@sap/ux-specification": "^1.108.8",
    "sqlite3": "^5"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-apptemplater": "cds watch --open apptemplater/webapp/index.html?sap-ui-xx-viewCache=false"
  },
  "cds": {
    "requires": {
      "db": {
        "kind": "sqlite",
        "credentials": {
          "database": "db.sqlite"
        }
      }
    },
    "fiori": {
      "lean_draft": true
    }
  },
  "sapux": [
    "app/apptemplater"
  ]
}
